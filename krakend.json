{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": "3",
  "name": "BiopatternGateway",
  "extra_config": {},
  "timeout": "5000ms",
  "cache_ttl": "0s",
  "output_encoding": "no-op",
  "port": 9000,
  "endpoints": [
    {
      "endpoint": "/api/search-objects/blat/search-option",
      "method": "POST",
      "output_encoding": "no-op",
      "extra_config": {},
      "backend": [
        {
          "url_pattern": "/blat/search-options",
          "host": ["http://search-biological-objects:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
        {
      "endpoint": "/api/search-objects/transcription-factor/by-promoter-region",
      "method": "POST",
      "output_encoding": "no-op",
      "extra_config": {},
      "backend": [
        {
          "url_pattern": "/transcription-factor/by-promoter-region",
          "host": ["http://search-biological-objects:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/search-objects/transcription-factor/by-blat-coordinates",
      "method": "POST",
      "output_encoding": "no-op",
      "extra_config": {},
      "backend": [
        {
          "url_pattern": "/transcription-factor/by-blat-coordinates",
          "host": ["http://search-biological-objects:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
     {
      "endpoint": "/api/biological-object/search/{type}/{value}",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {},
      "backend": [
        {
          "url_pattern": "/biological-object/search/{type}/{value}",
          "host": ["http://search-biological-objects:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/biological-object/complex/{value}",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {},
      "backend": [
        {
          "url_pattern": "/biological-object/complex/{value}",
          "host": ["http://search-biological-objects:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/integrations/pdb/find-ids",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {},
      "input_query_strings": [
        "symbol"
      ],
      "backend": [
        {
          "url_pattern": "/integrations/pdb/find-ids",
          "host": ["http://integrations:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/integrations/pdb/search",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {},
      "input_query_strings": [
        "pdbId"
      ],
      "backend": [
        {
          "url_pattern": "/integrations/pdb/search",
          "host": ["http://integrations:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/config-and-control/networks",
      "method": "POST",
      "output_encoding": "no-op",
      "extra_config": {},
      "input_headers": [
        "Content-Type",
        "x-user-id"
      ],
      "backend": [
        {
          "url_pattern": "/config-and-control/networks",
          "host": ["http://config-and-control:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/config-and-control/networks",
      "method": "PUT",
      "output_encoding": "no-op",
      "extra_config": {},
      "input_headers": [
        "Content-Type",
        "x-user-id"
      ],
      "backend": [
        {
          "url_pattern": "/config-and-control/networks",
          "host": ["http://config-and-control:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/config-and-control/pipelines/launch",
      "method": "POST",
      "output_encoding": "no-op",
      "extra_config": {},
      "input_query_strings": [
        "id"
      ],
      "input_headers": [
        "Content-Type",
        "x-user-id"
      ],
      "backend": [
        {
          "url_pattern": "/config-and-control/pipelines/launch",
          "host": ["http://config-and-control:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/config-and-control/pipelines",
      "method": "POST",
      "output_encoding": "no-op",
      "extra_config": {},
      "input_headers": [
        "Content-Type",
        "x-user-id"
      ],
      "backend": [
        {
          "url_pattern": "/config-and-control/pipelines",
          "host": ["http://config-and-control:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/api/config-and-control/pipelines",
      "method": "PUT",
      "output_encoding": "no-op",
      "extra_config": {},
      "input_headers": [
        "Content-Type",
        "x-user-id"
      ],
      "backend": [
        {
          "url_pattern": "/config-and-control/pipelines",
          "host": ["http://config-and-control:8080"],
          "encoding": "no-op",
          "sd": "static",
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}